#include <stdio.h>
#include <stdlib.h>
#include <Math.h>





int main()
{
    while(1){
         float x,y;
         printf("Tabani giriniz: \n");
         scanf("%f",&x);
         printf("ussu giriniz: \n");
         scanf("%f",&y);
         printf("Sonuc =%f \n\n",exp(y*log(x)));
         }

    return 0;
}


