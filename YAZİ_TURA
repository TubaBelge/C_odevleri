#include <stdio.h>
#include <stdlib.h>

int main()
{
    int yazi=0;
    int tura=0;
    int random=0;
        srand(time(NULL));

    for(int i=0; i<=10; i++)
    {
        random=rand()%10;

        if(random<=5)
        {
            yazi++;
        }
        else
        {
            tura++;
        }
    }
    printf("yazi:%d  ve  tura:%d",yazi,tura);





}
